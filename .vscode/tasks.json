{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++.exe 生成活动文件",
            "command": "F:\\mingw64\\bin\\g++.exe",
            "args": [
                "-g",
                "${file}",
                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe",
                "-I",
                "F:\\OpenCV\\build\\install\\include",
                "-I",
                "F:\\OpenCV\\build\\install\\include\\opencv2",
                "-L", "F:\\OpenCV\\build\\lib",
                "-l", "libopencv_core451",
                "-l", "libopencv_imgproc451",
                "-l", "libopencv_video451",
                "-l", "libopencv_ml451",
                "-l", "libopencv_highgui451",
                "-l", "libopencv_objdetect451",
                "-l", "libopencv_flann451",
                "-l", "libopencv_imgcodecs451",
                "-l", "libopencv_photo451",
                "-l", "libopencv_videoio451"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ],
    "version": "2.0.0"
}
